{"ast":null,"code":"import { defineComponent } from 'vue';\nimport { mapGetters, mapActions } from 'vuex';\nexport default defineComponent({\n  name: 'DashboardView',\n  data() {\n    return {\n      form: {\n        title: '',\n        content: ''\n      }\n    };\n  },\n  created: function () {\n    return this.$store.dispatch('getNotes');\n  },\n  computed: {\n    ...mapGetters({\n      notes: 'stateNotes'\n    })\n  },\n  methods: {\n    ...mapActions(['createNote']),\n    async submit() {\n      await this.createNote(this.form);\n    }\n  }\n});","map":{"version":3,"mappings":"AAoDA,SAASA,eAAc,QAAS,KAAK;AACrC,SAASC,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAeF,eAAe,CAAC;EAC7BG,IAAI,EAAE,eAAe;EACrBC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE,YAAW;IAClB,OAAO,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,UAAU,CAAC;EACzC,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGV,UAAU,CAAC;MAAEW,KAAK,EAAE;IAAY,CAAC;EACtC,CAAC;EACDC,OAAO,EAAE;IACP,GAAGX,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC7B,MAAMY,MAAM,GAAG;MACb,MAAM,IAAI,CAACC,UAAU,CAAC,IAAI,CAACV,IAAI,CAAC;IAClC;EACF;AACF,CAAC,CAAC","names":["defineComponent","mapGetters","mapActions","name","data","form","title","content","created","$store","dispatch","computed","notes","methods","submit","createNote"],"sourceRoot":"","sources":["C:\\Data\\devops\\vue\\healthMon\\services\\frontend\\src\\views\\DashboardView.vue"],"sourcesContent":["<template>\n  <div>\n    <section>\n      <h1>Add new note</h1>\n      <hr/><br/>\n\n      <form @submit.prevent=\"submit\">\n        <div class=\"mb-3\">\n          <label for=\"title\" class=\"form-label\">Title:</label>\n          <input type=\"text\" name=\"title\" v-model=\"form.title\" class=\"form-control\" />\n        </div>\n        <div class=\"mb-3\">\n          <label for=\"content\" class=\"form-label\">Content:</label>\n          <textarea\n            name=\"content\"\n            v-model=\"form.content\"\n            class=\"form-control\"\n          ></textarea>\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n      </form>\n    </section>\n\n    <br/><br/>\n\n    <section>\n      <h1>Notes</h1>\n      <hr/><br/>\n\n      <div v-if=\"notes.length\">\n        <div v-for=\"note in notes\" :key=\"note.id\" class=\"notes\">\n          <div class=\"card\" style=\"width: 18rem;\">\n            <div class=\"card-body\">\n              <ul>\n                <li><strong>Note Title:</strong> {{ note.title }}</li>\n                <li><strong>Author:</strong> {{ note.author.username }}</li>\n                <li><router-link :to=\"{name: 'Note', params:{id: note.id}}\">View</router-link></li>\n              </ul>\n            </div>\n          </div>\n          <br/>\n        </div>\n      </div>\n\n      <div v-else>\n        <p>Nothing to see. Check back later.</p>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\nimport { mapGetters, mapActions } from 'vuex';\n\nexport default defineComponent({\n  name: 'DashboardView',\n  data() {\n    return {\n      form: {\n        title: '',\n        content: '',\n      },\n    };\n  },\n  created: function() {\n    return this.$store.dispatch('getNotes');\n  },\n  computed: {\n    ...mapGetters({ notes: 'stateNotes'}),\n  },\n  methods: {\n    ...mapActions(['createNote']),\n    async submit() {\n      await this.createNote(this.form);\n    },\n  },\n});\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}